<!-- stats content start -->
<div class="tab-pane" id="tab2">
    <div class="row show-grid embeded_padding">

        <h1 class="stats_heading">Stats</h1>
        <div class="stats_background">
            <div class="row-fluid show-grid">
                <div class="span12">
