                </div><!-- span11 -->
            </div><!-- row -->
        </div><!-- stats_background -->
        <div class="stats_background">

            <div class="row-fluid show-grid">
                <div class="span3 offset3"></div>
              <div class="span3 offset3">

                <div id="time-ranges">
            <%= link_to '1 Day', bars_path( days: 1), class: 'btn', remote: true, onclick: 'timeRangesClick(this)' %>
            <%= link_to '7 Days', bars_path( days: 7), class: 'btn', remote: true, onclick: 'timeRangesClick(this)' %>
            <%= link_to '30 Days', bars_path( days: 30), class: 'btn', remote: true, onclick: 'timeRangesClick(this)', disabled: 'true' %>
        </div>
              </div>
            </div>
    </div>

        <div class="stats_background">
            <div class="navbar">
              <div class="navbar-inner statsnav_background">
                <ul class="nav">
                    <li>
                        <%= link_to "Name", "#" %>
                    </li>
                </ul>

                <ul class="nav pull-right">
                    <li class="divider-vertical"></li>
                    <li><a href="#">Visits</a></li>
                    <li class="divider-vertical"></li>
                    <li><a href="#">Hits</a></li>
                    <li class="divider-vertical"></li>
                    <li><a href="#">Hitthrough Rate</a></li>

                </ul>
            </div>
            <div id="stat-boxes">
                <%= render partial: 'stat_box', collection: @bars %>
            </div>

        </div>
    </div>

